{
  "name": "playproof-sdk",
  "version": "0.1.0",
  "description": "PlayProof SDK - Game-based human verification with behavior analysis",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/types/integrations/react.d.ts",
        "default": "./dist/esm/integrations/react.js"
      },
      "require": {
        "types": "./dist/types/integrations/react.d.ts",
        "default": "./dist/cjs/integrations/react.cjs"
      }
    },
    "./vue": {
      "import": {
        "types": "./dist/types/integrations/vue.d.ts",
        "default": "./dist/esm/integrations/vue.js"
      },
      "require": {
        "types": "./dist/types/integrations/vue.d.ts",
        "default": "./dist/cjs/integrations/vue.cjs"
      }
    },
    "./web-component": {
      "import": {
        "types": "./dist/types/web-component.d.ts",
        "default": "./dist/esm/web-component.js"
      },
      "require": {
        "types": "./dist/types/web-component.d.ts",
        "default": "./dist/cjs/web-component.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/rename-cjs.js",
    "build:types": "tsc -p tsconfig.types.json",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "captcha",
    "verification",
    "bot-detection",
    "human-verification",
    "game-captcha",
    "behavior-analysis",
    "playproof"
  ],
  "author": "PlayProof Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asaha96/Playproof.git"
  },
  "bugs": {
    "url": "https://github.com/asaha96/Playproof/issues"
  },
  "homepage": "https://github.com/asaha96/Playproof#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
